            <Target action="show" conditionResult="true" option="lowerRidgeRegularization"/>
            <Target action="hide" conditionResult="false" option="lowerRidgeRegularization"/>
            <Target action="show" conditionResult="true" option="upperRidgeRegularization"/>
            <Target action="hide" conditionResult="false" option="upperRidgeRegularization"/>
